{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/public/index.html" },
    { "src": "/api/install", "dest": "/api/install.php" },
    { "src": "/api/migrate", "dest": "/api/migrate.php" },
    { "src": "/static/ybold", "dest": "/static/YonseiBold.ttf" },
    { "src": "/static/ylight", "dest": "/static/YonseiLight.ttf" }
  ]
}